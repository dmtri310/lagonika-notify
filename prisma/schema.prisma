generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id         Int     @id @default(autoincrement())
  post_id    Int     @unique
  title      String
  desc       String?
  price      String?
  image      String
  author     String
  is_expired Boolean @default(false)
}
